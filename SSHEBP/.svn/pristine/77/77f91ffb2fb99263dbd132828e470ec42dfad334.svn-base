package com.ebp.exception;

public class BookAddExecption extends Exception{

	private static final long serialVersionUID = 1L;

	public BookAddExecption() {
		super();
	}

	public BookAddExecption(String message) {
		super(message);
	}

	public BookAddExecption(Throwable cause) {
		super(cause);
	}
	

}
